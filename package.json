{
	"name": "micro-meta-app-omero",
	"version": "1.3.1-b1",
	"private": true,
	"main": "./main.js",
	"publishConfig": {
		"access": "restricted"
	},
	"dependencies": {
		"micro-meta-app-react": "github:WU-BIMAC/MicroMetaApp-React#master",
		"react": "^16.14.0",
		"react-bootstrap": "1.4.0",
		"react-dom": "^16.14.0"
	},
	"scripts": {
		"test": "node scripts/test.js",
		"buildProd": "gulp build-prod",
		"buildDev": "gulp build-dev",
		"dev": "gulp dev",
		"build": "gulp build-prod && gulp build-dev",
		"update-mma": "npm uninstall micro-meta-app-react && npm cache clean --force && npm install micro-meta-app-react@github:WU-BIMAC/MicroMetaApp-React#master --save"
	},
	"build": {},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-react": "^7.12.13",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"babel-preset-react-app": "^9.1.2",
		"case-sensitive-paths-webpack-plugin": "2.3.0",
		"css-loader": "4.2.1",
		"dotenv": "8.2.0",
		"dotenv-expand": "5.1.0",
		"eslint": "7.7.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-flowtype": "5.2.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-react": "7.20.6",
		"fancy-log": "^1.3.3",
		"fs": "0.0.1-security",
		"gulp": "^4.0.2",
		"html-webpack-plugin": "4.3.0",
		"jest": "26.4.0",
		"jest-pnp-resolver": "1.2.2",
		"jest-resolve": "26.4.0",
		"jest-watch-typeahead": "^0.6.1",
		"mini-css-extract-plugin": "0.10.0",
		"optimize-css-assets-webpack-plugin": "5.0.3",
		"plugin-error": "^1.0.1",
		"pnp-webpack-plugin": "1.6.4",
		"postcss-flexbugs-fixes": "4.2.1",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.7.0",
		"postcss-safe-parser": "4.0.2",
		"react-dev-utils": "^11.0.4",
		"rm-local-modules": "0.0.2",
		"sass-loader": "9.0.3",
		"style-loader": "1.2.1",
		"typescript": "^3.9.9",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12",
		"workbox-webpack-plugin": "5.1.3"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
